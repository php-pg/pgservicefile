{
    "name": "php-pg/pgservicefile",
    "description": "Parser for PostgreSQL .pg_service.conf file",
    "keywords": [
        "postgres",
        "postgresql",
        "pg_service",
        "file",
        "parser"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry K.",
            "email": "coder1994@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "amphp/byte-stream": "^2.0.0"
    },
    "autoload": {
        "psr-4": {
            "PhpPg\\PgServiceFile\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpPg\\PgServiceFile\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.4",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6"
    }
}
